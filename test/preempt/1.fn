#!/bin/awk -f

function abs(v) {
	return v < 0 ? -v : v;
}

{
        s[$3]++
	print
}

2 < abs($6 - $8) {
	print "TIME FAILED"
	ecode = 1
}

$3 != last { 
	if (last && 10 < abs($8 - change - 100)) {
		print "PREEMPT FAILED", $8, change
		ecode = 1
	}
	last = $3
	change = $8
}

END {
        r = s[1] / s[2]
        d = 0.2
	if (r < 1 - d || 1 + d < r) {
		ecode = 1
	}
	exit(ecode)
}

